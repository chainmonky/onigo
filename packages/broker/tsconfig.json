{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "rootDir": "demo-scripts",
    "declaration": true,
    "resolveJsonModule": true
  },
  "include": ["demo-scripts/**/*"]
}
